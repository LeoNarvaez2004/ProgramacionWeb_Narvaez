<div class="input-group input-group-sm mb-3">
    <span class="input-group-text" id="inputGroup-sizing-sm">Tipo Medicamento nombre</span>
    <input type="text" id="nombreTipoMed" onkeyup="filtraTipoMed()" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-sm">
</div>
<div id="divTabla">
</div>

<div class="col-12">
    <button onclick="Limpiar()" class="btn btn-primary" type="button">Limpiar</button>
</div>

<fieldset>
    <legend>Buscar laboratorio</legend>
    <form id="frmBusqueda" method="POST">
        <div class="input-group input-group-sm mb-3">
            <span class="input-group-text" id="inputGroup-sizing-sm">Id Tipo Medicamento</span>
            <input type="text" name="idTipoMedicamento" id="idTipoMedicamento" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-sm">
        </div>
        <div class="input-group input-group-sm mb-3">
            <span class="input-group-text" id="inputGroup-sizing-sm">Nombre</span>
            <input type="text" name="nombre" id="nombre" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-sm">
        </div>
        <div class="input-group input-group-sm mb-3">
            <span class="input-group-text" id="inputGroup-sizing-sm">Descripcion</span>
            <input type="text" name="descripcion" id="descripcion" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-sm">
        </div>
        <button type="button" onclick="guardarTipoMed()" class="btn btn-outline-primary">Guardar</button>
        <div id="divTabla">
            <div id="divContenedor"></div>
        </div>
        <div class="col-12">
            <button onclick="LimpiarTipoMed()" class="btn btn-primary" type="button">Limpiar</button>
        </div>
    </form>
</fieldset>

<script src="~/js/queries.js"></script>
<script src="~/js/tipomedicamento.js"></script>